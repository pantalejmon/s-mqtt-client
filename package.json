{
  "name": "s-mqtt-client",
  "version": "0.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "build-clean": "npm run clean && tsc",
    "postinstall": "npm run build",
    "go": "tsc && node dist/index.js",
    "dev": "nodemon -e ts  --exec \"npm run go\"",
    "watch": "tsc --watch",
    "test": "mocha --opts tests/mocha.opts",
    "test-watch": "mocha --opts tests/mocha.opts --watch",
    "clean": "rm -rf dist",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint-fix": "tslint -c tslint.json 'src/**/*.ts' --fix"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.18",
    "eslint": "^7.4.0",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6"
  },
  "keywords": [
    "MQTT",
    "mqtt",
    "security",
    "light security",
    "iot",
    "ecc",
    "crypto",
    "secure communication",
    "secure iot"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pantalejmon/s-mqtt"
  },
  "dependencies": {
    "@types/eccrypto": "^1.1.1",
    "eccrypto": "^1.1.3",
    "mqtt": "^4.1.0"
  }
}
